<!DOCTYPE html>
<html>
<head>
<%= stylesheet_link_tag "application" %>
<%= javascript_include_tag "application" %>
</head>
<body>
	<%= render "layouts/header" %>
	
	<div class="container">
		<% flash.each do |name,msg| %>
			<%=content_tag(:div,msg,class: "alert alert-#{name}") %>
			<% end %> 
		<%= yield %>
		<%= render "layouts/footer" %>
	</div>

</body>
</html>
